<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
		<record id="view_op_crm_lead_analysis_form" model="ir.ui.view">
			<field name="name">op.crm.lead.analysis.form</field>
			<field name="model">op.crm.lead.analysis.xls</field>
			<field name="priority" eval="8" />
			<field name="arch" type="xml">
                <form string="Generate Lead Analysis Report" version="7.0">
                    <sheet>
                        <separator string="Select Date Range" colspan="4" />
                        <div>
                            <group colspan="4" cols="4">
                                <field name="start_date" />
                                <field name="end_date" />
                                <field name="study_programme_id"/>
                            </group>
                        </div>
                        <button icon="gtk-execute" name="xls_export" string="Generate Report" type="object" class="oe_highlight"
                                context="{'xls_export':1}"/>
                        <button special="cancel" string="Close" type="object"
                                icon="terp-dialog-close" class="oe_highlight"/>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="act_open_op_crm_lead_analysis_view">
            <field name="name">Generate Lead Analysis Report</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">op.crm.lead.analysis.xls</field>
			<field name="view_type">form</field>
			<field name="view_mode">form</field>
			<field name="target">new</field>
			<field name="domain">[]</field>
			<field name="context">{}</field>
		</record>

		<record model="ir.actions.act_window.view" id="act_open_op_crm_lead_analysis_view_form">
			<field name="act_window_id" ref="act_open_op_crm_lead_analysis_view" />
			<field name="sequence" eval="20" />
			<field name="view_mode">form</field>
			<field name="view_id" ref="view_op_crm_lead_analysis_form" />
		</record>

	</data>
</openerp>